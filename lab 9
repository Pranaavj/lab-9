public class Post
 {  
	protected User user; 
	private String date; 	
private String time; 
 	public Post(User user, String date, String time)
 { 	
	super(); 
		this.user = user; 
		this.date = date; 
		this.time = time; 
	}  	
public User getUser() 
{ 		

return user; 	
}  	
public String getDate() 
{ 	
	return date; 
	}  	
public String getTime()

 { 	
	return time; 	
}  	
public void setUser(User user)
 { 		
this.user = user; 	
}  	
public void setDate(String date)
 { 		
this.date = date; 
	}  
	public void setTime(String time)
 { 		
this.time = time; 
	} } 
